{
  "name": "next-horde",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:deploy": "npm run build && npm run deploy:prod",
    "deploy:prod": "node deploy-release.js --prod",
    "deploy:staging": "node deploy-release.js --staging",
    "start": "next start",
    "lint": "next lint",
    "pm2:start-prod": "pm2 start ecosystem.config.js --only horde-js",
    "pm2:stop-prod": "pm2 delete ecosystem.config.js --only horde-js",
    "pm2:start-staging": "pm2 start ecosystem.config.js --only horde-js-staging",
    "pm2:stop-staging": "pm2 delete ecosystem.config.js --only horde-js-staging"
  },
  "dependencies": {
    "@types/react-lazyload": "3.2.0",
    "@types/sharp": "0.31.0",
    "dexie": "3.2.2",
    "dexie-react-hooks": "1.1.1",
    "dotenv": "16.0.3",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-lazyload": "3.2.0",
    "sharp": "0.31.1",
    "ssh-deploy-release": "4.0.1"
  },
  "devDependencies": {
    "@types/node": "18.8.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "autoprefixer": "^10.4.12",
    "eslint": "8.25.0",
    "eslint-config-next": "12.3.1",
    "postcss": "8.4.17",
    "tailwindcss": "3.1.8",
    "typescript": "4.8.4"
  }
}