{
  "name": "next-horde",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze:prod": "ANALYZE=true NODE_ENV=production npm run build",
    "analyze:dev": "ANALYZE=true npm run dev",
    "dev": "npm run update:build-id && PORT=3000 node server.js",
    "build": "npm run update:build-id && next build",
    "deploy:prod": "npm run build && node ./x-build-scripts/release-deploy.js --prod",
    "deploy:staging": "npm run build && node ./x-build-scripts/release-deploy.js --staging",
    "lint": "next lint",
    "pm2:start-prod": "pm2 start ecosystem.config.js --only horde-js",
    "pm2:stop-prod": "pm2 delete ecosystem.config.js --only horde-js",
    "pm2:start-staging": "pm2 start ecosystem.config.js --only horde-js-staging",
    "pm2:stop-staging": "pm2 delete ecosystem.config.js --only horde-js-staging",
    "rollback:prod": "node ./x-build-scripts/release-rollback.js --prod",
    "rollback:staging": "node ./x-build-scripts/release-rollback.js --staging",
    "start": "NODE_ENV=production node server.js",
    "update:build-id": "node ./x-build-scripts/updateBuildId.js"
  },
  "dependencies": {
    "@next/bundle-analyzer": "13.1.1",
    "autosize": "5.0.2",
    "browser-image-resizer": "2.4.1",
    "client-zip": "2.3.0",
    "dexie": "3.2.2",
    "dexie-react-hooks": "1.1.1",
    "dotenv": "16.0.3",
    "express": "4.18.2",
    "fabric": "5.3.0",
    "fabric-history": "1.7.0",
    "file-saver": "2.0.5",
    "lz-string": "1.4.4",
    "next": "13.1.1",
    "next-dark-mode": "3.0.0",
    "node-abort-controller": "3.0.1",
    "node-fetch": "2.6.7",
    "nosleep.js": "0.12.0",
    "rc-slider": "10.1.0",
    "react": "18.2.0",
    "react-color": "2.19.3",
    "react-dom": "18.2.0",
    "react-dropzone": "14.2.3",
    "react-lazyload": "3.2.0",
    "react-select": "5.7.0",
    "react-sliding-pane": "7.3.0",
    "react-swipeable": "7.0.0",
    "react-switch": "7.0.0",
    "react-toastify": "9.1.1",
    "sharp": "0.31.3",
    "statery": "0.6.3",
    "styled-components": "5.3.6"
  },
  "devDependencies": {
    "@types/autosize": "4.0.1",
    "@types/fabric": "4.5.14",
    "@types/file-saver": "2.0.5",
    "@types/lz-string": "1.3.34",
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@types/react-lazyload": "3.2.0",
    "@types/sharp": "0.31.0",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "autoprefixer": "10.4.13",
    "babel-plugin-styled-components": "2.0.7",
    "eslint": "8.30.0",
    "eslint-config-next": "13.1.1",
    "postcss": "8.4.20",
    "ssh-deploy-release": "4.0.1",
    "tailwindcss": "3.2.4",
    "typescript": "4.9.4"
  }
}
